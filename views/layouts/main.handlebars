</div>

<!-- Modal -->
<div id="signin" class="modal fade" role="dialog">
    <div class="modal-dialog modal-sm">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Log In</h4>
            </div>
            <div class="modal-body">
                <span class="sp_error"></span>
                <div class="form-group">
                    <input type="text" class="form-control" id="sn_email" placeholder="Email">

                </div>
                <div class="form-group">
                    <input type="password" class="form-control" id="sn_pass" placeholder="Password">
                </div>
                <div class="form-group">
                    <input type="button" class="form-control" value="Log In" onclick="signin()">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>

<!-- Modal -->
<div id="signup" class="modal fade" role="dialog">
    <div class="modal-dialog modal-sm">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Sign Up</h4>
            </div>
            <div class="modal-body">
                <span class="sp_error"></span>
                <div class="form-group">
                    <input type="text" class="form-control" id="sp_fName" placeholder="First Name">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="sp_lName" placeholder="Last Name">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="sp_email" placeholder="Email">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" id="sp_pass" placeholder="Password">
                </div>
                <div class="form-group">
                    <input type="password" class="form-control" id="sp_cn_pass" placeholder="Confirm Password">
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="sp_company" placeholder="Company Name">
                </div>
                <div class="form-group">
                    <input type="button" class="form-control" value="Sign Up" onclick="signup()">
                </div>
            </div>


        </div>

    </div>

</div>

</div>
</div>
</body>


</html>
